        "def is_james_bond(values):\n",
        "  zero_zero_seven = []\n",
        "  for x in values:\n",
        "    if x == 0 or x == 7:\n",
        "      zero_zero_seven.append(x)\n",
        "  sample = [0, 0, 7]\n",
        "  if zero_zero_seven == sample:\n",
        "    return True\n",
        "  else:\n",
        "    return False\n",
        "is_james_bond( [1, 0, 2, 0, 4, 7, 5] )"
      ]
