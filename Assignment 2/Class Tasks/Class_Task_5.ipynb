      "source": [
        "def time(day):\n",
        "  years = (day // 365)\n",
        "  months = (day - (years * 365)) // 30\n",
        "  days = (day - (years * 365) - (months * 30))\n",
        "  print(f\"{years} years, {months} months and {days} days\")\n",
        "time(4000)"
      ]
