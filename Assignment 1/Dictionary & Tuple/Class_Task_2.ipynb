            "10\n",
            "20\n",
            "20\n",
            "30\n",
            "10\n",
            "50\n",
            "90\n",
            "STOP\n",
            "10 - 2 times\n",
            "20 - 2 times\n",
            "30 - 1 times\n",
            "50 - 1 times\n",
            "90 - 1 times\n"
          ]
        }
      ],
      "source": [
        "numbers = []\n",
        "result = {}\n",
        "while True:\n",
        "  inp = input()\n",
        "  if inp == \"STOP\":\n",
        "    break\n",
        "  else:\n",
        "    num = int(inp)\n",
        "    numbers.append(num)\n",
        "\n",
        "for x in numbers:\n",
        "  if x in result:\n",
        "    result[x] += 1\n",
        "  else:\n",
        "    result[x] = 1\n",
        "for key, value in result.items():\n",
        "  print(f\"{key} - {value} times\")"
      ]
    }
  ]
}